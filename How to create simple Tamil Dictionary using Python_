word = {'banana':'வாழைப்பழம',
            'milk':'பால்',
            'water':'நீர்',
            'rice':'சாதம',
            'coffee':'காப்பி',
            'fish':'மீன்',
            'meat':'இறைச்சி',  
            'fruit': 'பழம',
            'chicken':'கோழி',
            'tommato':'தக்காளி'}

inp = input('Enter Word:')
if inp in word:
    print('{} means {}'.format(inp.title(),word[inp]))
else:
    print('Word not fonud')
