coutries_currency = {}
for i,j in countries.items():
    for k,l in cur.items():
        if i == k:
            coutries_currency[i] = [ j,l ]
print('{:^6} {:>8} {:>43}'.format('CODE','Country','Currency'))
print('*'*60)
for x,y in coutries_currency.items():
    print('{:^7} {:44} {:4} '.format(x,y[0],y[1]))
