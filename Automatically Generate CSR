#!/usr/bin/python3
import pexpect

child = pexpect.spawn ('openssl req -nodes -newkey rsa:2048 -sha256 -keyout hostname.csr -out hostname.key ')
child.expect ('.*')
child.sendline ('IN')
child.expect ('.*')
child.sendline ('Tamilnadu')
child.expect ('.*')
child.sendline ('Chennai')
child.expect('.*')
child.sendline ('xyz Pvt Ltd')
child.expect('.*')
child.sendline ('Software')
child.expect('.*')
child.sendline ('xyz.xyz.com')
child.expect('.*')
child.sendline ('vijay@xyxz.com')
child.expect('.*')
child.sendline ('axy234@kl*')
child.expect('.*')
child.sendline ('Manaran Company')
